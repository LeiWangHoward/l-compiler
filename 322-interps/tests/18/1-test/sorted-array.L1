(
    (
        (esp -= 8)
        (eax <- (allocate 11 1))
        ((mem esp 4) <- eax)

        ((mem eax 4) <- 9)
        ((mem eax 8) <- 21)
        ((mem eax 12) <- 11)
        ((mem eax 16) <- 19)
        ((mem eax 20) <- 31)
        ; array = {4, 10, 5, 9, 15}

        (eax <- (print eax))
        (eax <- (mem esp 4))
        (call :sorted)
        (eax <- (print eax))

        (eax <- (allocate 11 1))
        ((mem esp 0) <- eax)

        ((mem eax 4) <- 7)
        ((mem eax 8) <- 13)
        ((mem eax 12) <- 17)
        ((mem eax 16) <- 21)
        ((mem eax 20) <- 23)
        ; array = {3, 6, 8, 10, 11}
        
        (eax <- (print eax))
        (eax <- (mem esp 0))
        (call :sorted)
        (eax <- (print eax))

        (esp += 8)
    )
    ( :sorted
        (esp -= 4)
        ((mem esp 0) <- ebp)
        (ebp <- esp)
        (esp -= 8)
        ((mem ebp -4) <- esi)
        ((mem ebp -8) <- edi)

        (ecx <- (mem eax 0))
        (ecx *= 2)
        (ecx += 1)
        (ebx <- 3)
    :sorted_loop
        (eax += 4)
        (edx <- (mem eax 0))
        (edi <- (mem eax 4))
        (cjump edx <= edi :sorted_continue :unsorted_end)
    :sorted_continue
        (ebx += 2)
        (cjump ebx < ecx :sorted_loop :sorted_end)
    :sorted_end
        (eax <- 3)
        (esi <- (mem ebp -4))
        (edi <- (mem ebp -8))
        (esp <- ebp)
        (ebp <- (mem esp 0))
        (esp += 4)
        (return)
    :unsorted_end
        (eax <- 1)
        (esi <- (mem ebp -4))
        (edi <- (mem ebp -8))
        (esp <- ebp)
        (ebp <- (mem esp 0))
        (esp += 4)
        (return)
    )
)
